{
  "name": "argo-energy-solutions",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "legacy:analyze:energy": "echo '[DEPRECATED] Use py: scripts instead' && node backend/scripts/analysis/analyze-energy-data.js",
    "legacy:explore:channels": "echo '[DEPRECATED]' && node backend/scripts/data-collection/explore-channels.js",
    "legacy:analyze:wilson": "echo '[DEPRECATED]' && node backend/scripts/analysis/wilson-center-analysis.js",
    "legacy:unit:health": "echo '[DEPRECATED]' && node backend/scripts/diagnostics/unit-health-report.js",
    "legacy:diagnose:data": "echo '[DEPRECATED]' && node backend/scripts/diagnostics/diagnose-data-access.js",
    "legacy:check:daily": "echo '[DEPRECATED]' && bash backend/scripts/utilities/check-data-daily.sh",
    "legacy:export:csv": "echo '[DEPRECATED]' && node backend/scripts/utilities/export-to-csv.js",
    "legacy:export:wilson:raw": "echo '[DEPRECATED]' && node backend/scripts/data-collection/export-wilson-raw-monthly.js",
    "legacy:ingest:data": "echo '[DEPRECATED]' && node backend/scripts/data-collection/ingest-eniscope-data.js",
    "legacy:ingest:postgres": "echo '[DEPRECATED]' && node backend/scripts/data-collection/ingest-to-postgres.js",
    "py:ingest": "source venv/bin/activate && python backend/python_scripts/ingest/ingest_to_postgres.py --days 1 --site",
    "py:ingest:full": "source venv/bin/activate && python backend/python_scripts/ingest/ingest_to_postgres.py --days 90 --site",
    "py:ingest:historical": "source venv/bin/activate && python backend/python_scripts/ingest/historical_ingestion.py --site",
    "py:ingest:historical:custom": "source venv/bin/activate && python backend/python_scripts/ingest/historical_ingestion.py",
    "py:sync": "./backend/python_scripts/operations/daily_sync.sh",
    "py:setup-cron": "./backend/python_scripts/operations/setup_cron.sh",
    "py:logs": "tail -f logs/daily_sync.log",
    "py:report": "source venv/bin/activate && python backend/python_scripts/deliver/generate_weekly_report.py --site",
    "py:report:custom": "source venv/bin/activate && python backend/python_scripts/deliver/generate_weekly_report.py",
    "py:report:customer": "source venv/bin/activate && python backend/python_scripts/deliver/generate_customer_report.py --site",
    "py:report:customer:json": "source venv/bin/activate && python backend/python_scripts/deliver/generate_customer_report.py --json --site",
    "py:query": "source venv/bin/activate && python backend/python_scripts/analyze/query_energy_data.py",
    "py:test": "source venv/bin/activate && python backend/python_scripts/tests/test_analytics.py",
    "py:test:verbose": "source venv/bin/activate && python backend/python_scripts/tests/test_analytics.py --verbose",
    "py:validate": "source venv/bin/activate && python backend/python_scripts/govern/validate_data.py",
    "py:profile-data": "python3 backend/python_scripts/analytics/profile_data_health.py",
    "py:site-profile": "python3 backend/python_scripts/analytics/generate_site_profile.py",
    "py:api": "source venv/bin/activate && uvicorn backend.server.main:app --reload --port 8000",
    "py:test:analytics": "source venv/bin/activate && cd backend/python_scripts && python -m pytest tests/test_forecast.py tests/test_cost_model.py -v",
    "py:export:tableau": "source venv/bin/activate && python backend/python_scripts/deliver/export_for_tableau.py",
    "py:export:tableau:custom": "source venv/bin/activate && python backend/python_scripts/deliver/export_for_tableau.py --custom",
    "py:cleanup": "source venv/bin/activate && python backend/python_scripts/operations/cleanup_old_files.py --execute",
    "py:cleanup:dry-run": "source venv/bin/activate && python backend/python_scripts/operations/cleanup_old_files.py",
    "py:operations:health": "source venv/bin/activate && python backend/python_scripts/operations/check_ingestion_health.py",
    "py:diagnostic:neutral-current": "source venv/bin/activate && python backend/python_scripts/operations/diagnostic_neutral_current.py",
    "py:govern:quality": "source venv/bin/activate && python backend/python_scripts/govern/data_quality_report.py",
    "py:govern:quality:30d": "source venv/bin/activate && python backend/python_scripts/govern/data_quality_report.py --days 30",
    "py:ingest:test-auth": "source venv/bin/activate && python backend/python_scripts/ingest/test_auth_approaches.py",
    "py:ingest:diagnostic": "source venv/bin/activate && python backend/python_scripts/ingest/diagnostic_report.py",
    "py:ingest:test-working": "source venv/bin/activate && python backend/python_scripts/ingest/test_working_auth.py",
    "py:ingest:discover": "source venv/bin/activate && python backend/python_scripts/ingest/discover_api_endpoints.py",
    "db:migrate:timestamptz": "source venv/bin/activate && python backend/python_scripts/govern/run_migration.py",
    "db:migrate:meters": "source venv/bin/activate && python backend/python_scripts/govern/add_meters_table.py",
    "db:migrate:sites": "source venv/bin/activate && python backend/python_scripts/govern/create_sites_table.py",
    "db:migrate:electrical-health": "source venv/bin/activate && python backend/python_scripts/govern/add_electrical_health_columns.py",
    "py:report:electrical-health": "source venv/bin/activate && python backend/python_scripts/deliver/generate_electrical_health_report.py --site",
    "py:report:asset-health": "source venv/bin/activate && python backend/python_scripts/deliver/generate_asset_health_report.py --site",
    "py:sites": "source venv/bin/activate && python backend/python_scripts/operations/list_active_sites.py",
    "py:sites:json": "source venv/bin/activate && python backend/python_scripts/operations/list_active_sites.py --json",
    "db:check-schema": "source venv/bin/activate && python -c \"import psycopg2, os; from dotenv import load_dotenv; load_dotenv(); conn = psycopg2.connect(os.getenv('DATABASE_URL')); cur = conn.cursor(); cur.execute('SELECT table_name, column_name, data_type FROM information_schema.columns WHERE table_schema = \\'public\\' AND column_name LIKE \\'%time%\\' ORDER BY table_name, column_name'); print('\\nüîç Timestamp Columns:\\n'); [print(f\\\"  {row[0]:20} {row[1]:25} {row[2]}\\\") for row in cur.fetchall()]; cur.close(); conn.close()\"",
    "db:views": "source venv/bin/activate && python backend/python_scripts/govern/run_create_views.py",
    "db:refresh-views": "source venv/bin/activate && python backend/python_scripts/govern/refresh_views.py",
    "legacy:db:check": "echo '[DEPRECATED]' && node backend/scripts/database/check-database.js",
    "legacy:db:test-neon": "echo '[DEPRECATED]' && node backend/scripts/database/test-neon-connection.js",
    "legacy:db:setup": "echo '[DEPRECATED]' && node backend/scripts/database/setup-postgres-schema.js",
    "legacy:db:migrate:sqlite-to-postgres": "echo '[DEPRECATED]' && node backend/scripts/database/migrate-sqlite-to-postgres.js",
    "legacy:api:server": "echo '[DEPRECATED] Use py:api instead'",
    "legacy:api:dev": "echo '[DEPRECATED] Use py:api instead'",
    "dev:all": "concurrently \"npm run py:api\" \"npm run dev\"",
    "start": "npm run dev:all",
    "legacy:report:weekly": "echo '[DEPRECATED] Use py:report instead' && node backend/scripts/reports/weekly-exceptions-brief.js",
    "legacy:report:test": "echo '[DEPRECATED]' && node backend/scripts/reports/tests/test-analytics.js",
    "legacy:report:html": "echo '[DEPRECATED]' && node backend/scripts/reports/generate-html-from-json.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.14.2",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^28.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
